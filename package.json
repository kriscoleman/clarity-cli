    "@types/promptly": "^3.0.2",
    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@types/selenium-webdriver": "4.1.5",
    "chalk": "4.1.2",
    "clear": "0.1.0",
    "commander": "9.4.1",
    "figlet": "1.5.2",
    "path": "0.12.7",
    "promptly": "^3.2.0",
    "selenium-webdriver": "4.5.0"
  },
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "clarity",
    "timesheets",
    "cli",
    "broadcom",
